package com.ezen.springmvc.stu;

public class Study {
	public static void main(String[] args) {
		
		Car car = new Car("모닝", 1000);
		
		
		System.out.println("여기에 자동차 이름이랑 자동차 가격을 출력해보세요");
		System.out.println("자동차의 이름: " + car.getCar_name());
		System.out.println("자동차의 가격: " + car.getCar_price());
		
		Car car1 = new Car("소나타", 1500);
		
		System.out.println("자동차의 이름: " + car1.getCar_name());
		System.out.println("자동차의 가격: " + car1.getCar_price());
		
		
		
	}

}
