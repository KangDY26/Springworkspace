package com.ezen.springdat.dto;

import java.util.Date;

import org.springframework.format.annotation.DateTimeFormat;

import lombok.Data;

@Data
public class FruitDTO {
	Integer fid;
	String fname;
	Integer price;
	Integer qty;
	@DateTimeFormat(pattern = "yyyy-MM-dd'T'HH:mm") // 2024-08-14T14:28
	Date exp_date;
	Integer location_id;
}
